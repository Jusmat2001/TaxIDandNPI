<Window x:Class="TaxIDandNPI.DeletePracticeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Delete Practice" Height="100" Width="250" WindowStartupLocation="CenterScreen" ResizeMode="CanMinimize" Closed="Window_Closed">
    <Grid Background="CornflowerBlue">
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <ComboBox x:Name="DeleteComboBox" Margin="5" Grid.ColumnSpan="2"/>
        <Button x:Name="DeleteBtn" Grid.Column="0" Margin="5" Content="Delete" Grid.Row="1" Click="DeleteBtn_Click"/>
        <Button x:Name="DeleteCancelBtn" Grid.Column="1" Margin="5" Content="Cancel" Grid.Row="1" Click="DeleteCancelBtn_Click"/>
    </Grid>
</Window>
